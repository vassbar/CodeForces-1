

import math
import numpy as np
import matplotlib.pyplot as plt


def fun(x):
    return 100000*(x - 0.9)**2 * (x - 1.1)**3



def fd(fun,x,h=0.01):
    der=(fun(x+h)-fun(x-h))/(2*h)
    return der


ntr=500
x1=1;
x2=0;

for i in range(ntr):
    x2=x1-fun(x1)/fd(fun,x1)
    print(i, x2,fun(x2))
    if(abs(x2-x1)<0.00001):
        print (i,x2,fun(x2))
        break

    x1=x2
if(i==ntr-1):
    print ("NO SOLUTION KALO GOOD MIKROULI")
